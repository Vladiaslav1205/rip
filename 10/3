using System;

namespace vvp3
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите A: ");
            int a = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Ответ: " + ((a % 2 == 0)&&(a / 10 < 10)));
        }
    }
}
