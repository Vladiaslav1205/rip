using System;

namespace vvp2
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите число: ");
            int a = Convert.ToInt32(Console.ReadLine());
            double c = 1;
            double i = 0;
            while (a != i)
            {
                c = c * (1 + i / 10);
                i++;
            }
                Console.WriteLine("Ответ: " + c);
        }
    }
}
