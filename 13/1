using System;

namespace vvp1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите цену одного кг конфет: ");
            double a = Convert.ToDouble(Console.ReadLine());
            for (double i = 1; i < 11; i++)
                Console.WriteLine("Цена за " + Convert.ToDouble(i/10) + " кг конфет: " + a * i / 10);
        }
    }
}
