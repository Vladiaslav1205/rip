using System;

namespace vvp3
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите целое число K (1-365): ");
            int a = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Введите число N (1-7): ");
            int b = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Номер дня недели: " + ((a + b - 2) % 7 + 1));
        }
    }
}
