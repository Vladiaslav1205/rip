using System;

namespace vvp2
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите значение угла в радианах как множитель  pi(...*pi): ");
            double a = Convert.ToDouble(Console.ReadLine());
            a = 180 * a;
            Console.WriteLine("Значение угла в градусах:" + a);
        }
    }
}
