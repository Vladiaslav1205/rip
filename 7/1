using System;

namespace vvp1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите значение угла в градусах: ");
            int a = Convert.ToInt32(Console.ReadLine());
            a = 180 / a;
            Console.WriteLine("Значение угла в радианах: pi/" + a);
        }
    }
}
