using System;

namespace vvp6
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите коэффициент A1: ");
            double a1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите коэффициент B1: ");
            double b1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите коэффициент C1: ");
            double c1 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите коэффициент A2: ");
            double a2 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите коэффициент B2: ");
            double b2 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("Введите коэффициент C2: ");
            double c2 = Convert.ToDouble(Console.ReadLine());
            Console.WriteLine("x = " + (c2 * b1 - c1 * b2) / (b1 * a2 - b2 * a1) + "; y = " + (c2 * a1 - c1 * a2) / (a1 * b2 -a2 * b1) );
        }
    }
}
