using System;

namespace vvp1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите числа A и B через пробел:");
            string[] A = Console.ReadLine().Split(' ');
            int a = int.Parse(A[0]);
            int b = int.Parse(A[1]);
            int c = a;
            a = b;
            b = c;
            Console.WriteLine("A = " + a + "; B = " + b);
        }
    }
}
