using System;

namespace vvp6
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите a: ");
            int a = Convert.ToInt32(Console.ReadLine());
            int b;
            b = a * a;
            b = b * b;
            b = b * b;
            Console.WriteLine("Ответ: " + b);
        }
    }
}
