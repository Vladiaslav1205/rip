using System;

namespace vvp5
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите трёхзначное число: ");
            int a = Convert.ToInt32(Console.ReadLine());
            a = a % 100;
            Console.WriteLine("Итог перестановки цифр: " + (a % 10 * 10 + a/10));
        }
    }
}
